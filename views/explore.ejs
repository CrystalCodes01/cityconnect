<h2> City Connect Explore </h2>

<ul>
  <% events.forEach((oneEvent)=>{ %>
    <li>
      <h3> <%= oneEvent.name %> </h3>
      <img class="photos" src='<%= oneEvent.photoUrl %>'>
      <br><br>
      <p>  <%= oneEvent.name %> </p>
      <p>  <%= oneEvent.address %>
           <%= oneEvent.city %>
           <%= oneEvent.state %>
           <%= oneEvent.zipcode %>
     </p>
      <p> <%= oneEvent.description %> </p>
      <a href="<%= oneEvent.url %>"><%= oneEvent.url %></a>
      <br><br>
      <p> Category: <%= oneEvent.category %> </p>
    </li>
  <% }); %>
</ul>
