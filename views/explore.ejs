    <!-- Page Content -->
    <div class="container">

        <!-- Page Heading/Breadcrumbs -->
        <div class="row">
            <div class="col-lg-12">
              <br>
                <h1 class="header"> Explore City Connect</h1>
            </div>
        </div>
        <hr class="hr-style">
        <!-- /.row -->
        <!-- Projects Row -->
        <div class="row">

                <a href="portfolio-item.html">
                  <% events.forEach((oneEvent)=>{ %>
                    <div class="col-md-4 img-portfolio height">
                  <img class="img-responsive img-hover photos" src='<%= oneEvent.photoUrl %>'>
                </a>
                    <h3> <%= oneEvent.name %> </h3>
                    <hr>
                    <p class="event-info">
                         <%= oneEvent.address %>
                         <%= oneEvent.city %>
                         <%= oneEvent.state %>
                         <%= oneEvent.zipcode %>
                   </p>
                   <hr>
                   <p> About: </p>
                   <p> <%= oneEvent.description %> </p>
                   <a href="<%= oneEvent.url %>"><%= oneEvent.url %></a>
                   <hr>
                   <p class="event-cat"> Category: <%= oneEvent.category %> </p>
                 </div>
                   <% }); %>
            </div>

            <br><br>
        <!-- Pagination -->
        <div class="row text-center">
            <div class="col-lg-12">
