<h2> Social Results </h2>

<p> Your results for <b> Social </b> search:</p>

<!-- <p> You searched for <i> <%= theSearch %> </i>. </p> -->

<ul>
  <% events.forEach((oneEvent)=>{ %>
    <li>
      <h3> <%= oneEvent.name %> </h3>
      <img class="photos" src='<%= oneEvent.photoUrl %>'>
      <p>  <%= oneEvent.name %> </p>
      <p>  <%= oneEvent.address %>
           <%= oneEvent.city %>
           <%= oneEvent.state %>
           <%= oneEvent.zipcode %>
     </p>
      <p> <%= oneEvent.description %> </p>
      <p> <%= oneEvent.url %> </p>
      <p> <%= oneEvent.category %> </p>
    </li>
  <% }); %>
</ul>
